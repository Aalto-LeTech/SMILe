#
# File ignoration patterns for Git
#
# The idea here is to explicitly ignore everything,
# except the exact files allowed to be checked in.
#

/*

!/.idea
!/project
!/smcl-bitmap-viewer
!/smcl-bitmap-viewer-tests
!/smcl-core
!/smcl-core-tests
!/smcl-public-interfaces
!/smcl-public-interfaces-tests
!/.gitattributes
!/.gitignore
!/build.sbt
!/CHANGELOG
!/CONTRIBUTING.md
!/LICENSE
!/README.md


/.idea/workspace.xml
/.idea/tasks.xml

/project/*
!/project/ApplicationDependencies.scala
!/project/build.properties
!/project/plugins.sbt

/smcl-bitmap-viewer/*
!/smcl-bitmap-viewer/js
!/smcl-bitmap-viewer/jvm
!/smcl-bitmap-viewer/shared
!/smcl-bitmap-viewer/root-doc.txt

/smcl-bitmap-viewer/js/*
/smcl-bitmap-viewer/jvm/*
/smcl-bitmap-viewer/shared/*
!/smcl-bitmap-viewer/js/src
!/smcl-bitmap-viewer/jvm/src
!/smcl-bitmap-viewer/shared/src

/smcl-bitmap-viewer-tests/*
!/smcl-bitmap-viewer-tests/js
!/smcl-bitmap-viewer-tests/jvm
!/smcl-bitmap-viewer-tests/shared
!/smcl-bitmap-viewer-tests/root-doc.txt

/smcl-bitmap-viewer-tests/js/*
/smcl-bitmap-viewer-tests/jvm/*
/smcl-bitmap-viewer-tests/shared/*
!/smcl-bitmap-viewer-tests/js/src
!/smcl-bitmap-viewer-tests/jvm/src
!/smcl-bitmap-viewer-tests/shared/src

/smcl-core/*
!/smcl-core/js
!/smcl-core/jvm
!/smcl-core/shared
!/smcl-core/root-doc.txt

/smcl-core/js/*
/smcl-core/jvm/*
/smcl-core/shared/*
!/smcl-core/js/src
!/smcl-core/jvm/src
!/smcl-core/shared/src

/smcl-core-tests/*
!/smcl-core-tests/js
!/smcl-core-tests/jvm
!/smcl-core-tests/shared
!/smcl-core-tests/root-doc.txt

/smcl-core-tests/js/*
/smcl-core-tests/jvm/*
/smcl-core-tests/shared/*
!/smcl-core-tests/js/src
!/smcl-core-tests/jvm/src
!/smcl-core-tests/shared/src

/smcl-public-interfaces/*
!/smcl-public-interfaces/js
!/smcl-public-interfaces/jvm
!/smcl-public-interfaces/shared
!/smcl-public-interfaces/root-doc.txt

/smcl-public-interfaces/js/*
/smcl-public-interfaces/jvm/*
/smcl-public-interfaces/shared/*
!/smcl-public-interfaces/js/src
!/smcl-public-interfaces/jvm/src
!/smcl-public-interfaces/shared/src

/smcl-public-interfaces-tests/*
!/smcl-public-interfaces-tests/js
!/smcl-public-interfaces-tests/jvm
!/smcl-public-interfaces-tests/shared
!/smcl-public-interfaces-tests/root-doc.txt

/smcl-public-interfaces-tests/js/*
/smcl-public-interfaces-tests/jvm/*
/smcl-public-interfaces-tests/shared/*
!/smcl-public-interfaces-tests/js/src
!/smcl-public-interfaces-tests/jvm/src
!/smcl-public-interfaces-tests/shared/src

**/.git/*
**/.history/*
**/__pycache__/*
**/.cache-main
**/.DS_Store
**/*.bak
**/*.class
**/*.log
**/*.pyc
**/*.pyo
**/*~
